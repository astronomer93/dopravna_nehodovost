#rok=2019
#mesiac=01
#den=01
#valueParameters=(46)
#count=0

rm data.txt && rm data1.txt && rm ok.txt

wget --post-data "activity=retrieve&res=hour&spacecraft=omni2&start_date=20100101&end_date=20161231&vars=24&scale=Linear&ymin=&ymax=&view=0&charsize=&xstyle=0&ystyle=0&symbol=0&symsize=&linestyle=solid&table=0&imagex=640&imagey=480&color=&back=" https://omniweb.sci.gsfc.nasa.gov/cgi/nx1.cgi -O data.txt -q
sed '1,8d' data.txt | awk '{print $4}' | sed '25,80d' >> data1.txt
awk '{if($1>2000) print $1}' data1.txt >> ok.txt 
	
