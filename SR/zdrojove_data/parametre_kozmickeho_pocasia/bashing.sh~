wget --post-data "activity=retrieve&res=hour&spacecraft=omni2&start_date=20161219&end_date=20161219&vars=40&scale=Linear&ymin=&ymax=&view=0&charsize=&xstyle=0&ystyle=0&symbol=0&symsize=&linestyle=solid&table=0&imagex=640&imagey=480&color=&back=" http://omniweb.gsfc.nasa.gov/cgi/nx1.cgi -O text.txt -q
sed '1,8d' text.txt >> text1.txt && awk '{print $4}' text1.txt >> text2.txt
rm text.txt && rm text1.txt && sed '25,80d' text2.txt >> text3.txt && rm text2.txt
awk '{sum +=$0} END {if(NR > 0) printf "%f\n", sum/NR}' text3.txt >> text4.txt && rm text3.txt

